<title><%= t "suggest"%></title>
<h1><%= t "user_suggest" %></h1>
<div class= "row">
  <div class= "col-md-6 col-md-offset-3">
    <%= form_for @suggest do |f| %>
      <%= f.text_area :content, class: "form-control" %>
      <%= f.submit t "send_suggest", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
